}

@keyframes pulse {
  0%, 100% { transform: scale(1); opacity: 0.5; }
  50% { transform: scale(1.1); opacity: 0.8; }
}

.donate-form .form-control {
  background-color: rgba(255, 255, 255, 0.15);
